[init]
defaultBranch = main

[user]
name = <your name>
email = <your email>

[core]
editor = code --wait

[alias]
st = !git status -s
cm = !git add --all && git commit -m
ck = !git checkout
l = !git log --pretty=format:'%C(blue)%h%C(red)%d %C(white)%s - %C(cyan)%cn, %C(green)%cr'

[pull]
rebase = false
